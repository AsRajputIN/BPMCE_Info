<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Home"
    android:background="#CAC9C9">


    <com.google.android.material.appbar.AppBarLayout
        android:id="@+id/toolbar1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <androidx.appcompat.widget.Toolbar
            android:id="@+id/toolbar"
            android:background="#FF8656"
            android:theme="@style/ThemeOverlay.AppCompat.Dark.ActionBar"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"/>

    </com.google.android.material.appbar.AppBarLayout>






    <androidx.core.widget.NestedScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/toolbar1"
        android:fillViewport="true">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"

            android:orientation="vertical">


            <TextView
                android:id="@+id/textView2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="14dp"
                android:shadowColor="#000000"
                android:text="Information"
                android:textColor="#000000"
                android:textSize="34sp"
                android:textStyle="bold" />

            <LinearLayout
                android:id="@+id/Linear1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/textView2"
                android:layout_marginTop="8dp"
                android:gravity="center"
                android:orientation="horizontal">

                <androidx.cardview.widget.CardView
                    android:id="@+id/timetablecard"
                    android:layout_width="175dp"
                    android:layout_height="160dp"
                    app:cardBackgroundColor="@color/white"
                    app:cardCornerRadius="9dp">

                    <ImageView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_marginBottom="17dp"
                        android:src="@drawable/time_table" />

                    <TextView
                        android:id="@+id/timetable"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:gravity="bottom"
                        android:text="Time Table"
                        android:textAlignment="center"
                        android:textColor="@color/black"
                        android:textSize="25dp"
                        android:textStyle="bold"
                        tools:ignore="RtlCompat" />
                </androidx.cardview.widget.CardView>


                <androidx.cardview.widget.CardView
                    android:id="@+id/noticecard"
                    android:layout_width="175dp"
                    android:layout_height="160dp"
                    android:layout_marginLeft="20dp"
                    app:cardBackgroundColor="@color/white"
                    app:cardCornerRadius="9dp">

                    <ImageView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_marginBottom="15dp"
                        android:src="@drawable/notice" />

                    <TextView
                        android:id="@+id/Notice"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:gravity="bottom"
                        android:text="Notice"
                        android:textAlignment="center"
                        android:textColor="@color/black"
                        android:textSize="25dp"
                        android:textStyle="bold"
                        tools:ignore="RtlCompat" />
                </androidx.cardview.widget.CardView>

            </LinearLayout>

            <LinearLayout
                android:id="@+id/Linear2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/Linear1"
                android:layout_marginTop="11dp"
                android:gravity="center"
                android:orientation="horizontal">

                <androidx.cardview.widget.CardView
                    android:id="@+id/resultcard"
                    android:layout_width="175dp"
                    android:layout_height="160dp"
                    app:cardBackgroundColor="@color/white"
                    app:cardCornerRadius="9dp">

                    <ImageView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_marginBottom="15dp"
                        android:src="@drawable/result" />

                    <TextView
                        android:id="@+id/result"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:gravity="bottom"
                        android:text="Result"
                        android:textAlignment="center"
                        android:textColor="@color/black"
                        android:textSize="25dp"
                        android:textStyle="bold"
                        tools:ignore="RtlCompat" />
                </androidx.cardview.widget.CardView>

                <androidx.cardview.widget.CardView
                    android:id="@+id/collegeinfocard"
                    android:layout_width="175dp"
                    android:layout_height="160dp"
                    android:layout_marginLeft="20dp"
                    app:cardBackgroundColor="@color/white"
                    app:cardCornerRadius="9dp">

                    <ImageView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_marginBottom="19dp"
                        android:src="@drawable/college_info" />

                    <TextView
                        android:id="@+id/collegeinfo"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:gravity="bottom"
                        android:text="College Info"
                        android:textAlignment="center"
                        android:textColor="@color/black"
                        android:textSize="25dp"
                        android:textStyle="bold"
                        tools:ignore="RtlCompat" />
                </androidx.cardview.widget.CardView>
            </LinearLayout>

            <LinearLayout
                android:id="@+id/Linear3"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/Linear2"
                android:layout_marginTop="11dp"
                android:gravity="center"
                android:orientation="horizontal">

                <androidx.cardview.widget.CardView
                    android:id="@+id/teacherinfocard"
                    android:layout_width="175dp"
                    android:layout_height="160dp"
                    app:cardBackgroundColor="@color/white"
                    app:cardCornerRadius="9dp">

                    <ImageView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_marginTop="9dp"
                        android:layout_marginBottom="28dp"
                        android:src="@drawable/teacher_info" />

                    <TextView
                        android:id="@+id/Teacherinfo"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:gravity="bottom"
                        android:text="Teacher Info"
                        android:textAlignment="center"
                        android:textColor="@color/black"
                        android:textSize="25dp"
                        android:textStyle="bold"
                        tools:ignore="RtlCompat" />
                </androidx.cardview.widget.CardView>

                <androidx.cardview.widget.CardView
                    android:id="@+id/placementinfocard"
                    android:layout_width="175dp"
                    android:layout_height="160dp"
                    android:layout_marginLeft="20dp"
                    app:cardBackgroundColor="@color/white"
                    app:cardCornerRadius="9dp">

                    <ImageView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_marginBottom="18dp"
                        android:src="@drawable/placement_info" />

                    <TextView
                        android:id="@+id/placementinfo"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:gravity="bottom"
                        android:text="Placement Info"
                        android:textAlignment="center"
                        android:textColor="@color/black"
                        android:textSize="23dp"
                        android:textStyle="bold"
                        tools:ignore="RtlCompat" />
                </androidx.cardview.widget.CardView>
            </LinearLayout>

            <TextView
                android:id="@+id/textclub"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/Linear3"
                android:layout_marginLeft="14dp"
                android:layout_marginTop="12dp"
                android:shadowColor="#000000"
                android:text="Club"
                android:textColor="#000000"
                android:textSize="34sp"
                android:textStyle="bold" />

            <LinearLayout
                android:id="@+id/linearclub"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/club"
                android:layout_marginTop="8dp"
                android:gravity="center"
                android:orientation="horizontal">

                <androidx.cardview.widget.CardView
                    android:id="@+id/codingclub"
                    android:layout_width="115dp"
                    android:layout_height="120dp"
                    app:cardBackgroundColor="@color/white"
                    app:cardCornerRadius="9dp">

                    <ImageView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_marginBottom="28dp"
                        android:layout_marginTop="9dp"
                        android:src="@drawable/coding_club"
                        />

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:gravity="bottom"
                        android:text="Coding club"
                        android:textAlignment="center"
                        android:textColor="@color/black"
                        android:textSize="19dp"
                        android:textStyle="bold"
                        tools:ignore="RtlCompat" />
                </androidx.cardview.widget.CardView>

                <androidx.cardview.widget.CardView
                    android:id="@+id/sportsclub"
                    android:layout_width="115dp"
                    android:layout_height="120dp"
                    android:layout_marginLeft="20dp"
                    app:cardBackgroundColor="@color/white"
                    app:cardCornerRadius="9dp">

                    <ImageView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_marginBottom="28dp"
                        android:layout_marginTop="9dp"
                        android:src="@drawable/sports_club"
                        />

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:gravity="bottom"
                        android:text="Sports club"
                        android:textAlignment="center"
                        android:textColor="@color/black"
                        android:textSize="19dp"
                        android:textStyle="bold"
                        tools:ignore="RtlCompat" />
                </androidx.cardview.widget.CardView>

                <androidx.cardview.widget.CardView
                    android:id="@+id/nature"
                    android:layout_width="115dp"
                    android:layout_height="120dp"
                    android:layout_marginLeft="20dp"
                    app:cardBackgroundColor="@color/white"
                    app:cardCornerRadius="9dp">

                    <ImageView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_marginBottom="28dp"
                        android:layout_marginTop="9dp"
                        android:src="@drawable/nature_club"
                        />

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:gravity="bottom"
                        android:text="Nature"
                        android:textAlignment="center"
                        android:textColor="@color/black"
                        android:textSize="19dp"
                        android:textStyle="bold"
                        tools:ignore="RtlCompat" />
                </androidx.cardview.widget.CardView>
            </LinearLayout>

            <LinearLayout
                android:id="@+id/linearclub2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/linearclub"
                android:layout_marginTop="8dp"
                android:gravity="center"
                android:orientation="horizontal"
                android:padding="7dp"
                android:paddingBottom="12dp">

                <androidx.cardview.widget.CardView
                    android:id="@+id/invotionclub"
                    android:layout_width="115dp"
                    android:layout_height="120dp"
                    app:cardBackgroundColor="@color/white"
                    app:cardCornerRadius="9dp">

                    <ImageView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_marginBottom="28dp"
                        android:layout_marginTop="9dp"
                        android:src="@drawable/innovation"
                        />

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:gravity="bottom"
                        android:text="Innovation Club"
                        android:textAlignment="center"
                        android:textColor="@color/black"
                        android:textSize="15dp"
                        android:textStyle="bold"
                        tools:ignore="RtlCompat" />
                </androidx.cardview.widget.CardView>

                <androidx.cardview.widget.CardView
                    android:id="@+id/hackingclub"
                    android:layout_width="115dp"
                    android:layout_height="120dp"
                    android:layout_marginLeft="20dp"
                    app:cardBackgroundColor="@color/white"
                    app:cardCornerRadius="9dp">

                    <ImageView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_marginBottom="28dp"
                        android:layout_marginTop="9dp"
                        android:src="@drawable/startup"
                        />

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:gravity="bottom"
                        android:text="StartUp club"
                        android:textAlignment="center"
                        android:textColor="@color/black"
                        android:textSize="19dp"
                        android:textStyle="bold"
                        tools:ignore="RtlCompat" />
                </androidx.cardview.widget.CardView>

                <androidx.cardview.widget.CardView
                    android:id="@+id/art"
                    android:layout_width="115dp"
                    android:layout_height="120dp"
                    android:layout_marginLeft="20dp"
                    app:cardBackgroundColor="@color/white"
                    app:cardCornerRadius="9dp">

                    <ImageView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_marginBottom="28dp"
                        android:layout_marginTop="9dp"
                        android:src="@drawable/art_club"
                        />

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:gravity="bottom"
                        android:text="Art Club"
                        android:textAlignment="center"
                        android:textColor="@color/black"
                        android:textSize="19dp"
                        android:textStyle="bold"
                        tools:ignore="RtlCompat" />
                </androidx.cardview.widget.CardView>
            </LinearLayout>

            <TextView
                android:id="@+id/textsyllbus"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/Linearclub2"
                android:layout_marginLeft="9dp"
                android:layout_marginTop="12dp"
                android:shadowColor="#000000"
                android:text="Syllabus"
                android:textColor="#000000"
                android:textSize="34sp"
                android:textStyle="bold" />

            <LinearLayout
                android:id="@+id/linearsyllbus"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/club"
                android:layout_marginTop="8dp"
                android:gravity="center"
                android:orientation="horizontal">

                <androidx.cardview.widget.CardView
                    android:id="@+id/CSE"
                    android:layout_width="155dp"
                    android:layout_height="140dp"
                    app:cardBackgroundColor="@color/white"
                    app:cardCornerRadius="9dp">

                    <ImageView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_marginBottom="28dp"
                        android:layout_marginTop="9dp"
                        android:src="@drawable/cse"
                        />

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:gravity="bottom"
                        android:text="CSE"
                        android:textAlignment="center"
                        android:textColor="@color/black"
                        android:textSize="22dp"
                        android:textStyle="bold"
                        tools:ignore="RtlCompat" />
                </androidx.cardview.widget.CardView>

                <androidx.cardview.widget.CardView
                    android:id="@+id/Civil"
                    android:layout_width="155dp"
                    android:layout_height="140dp"
                    android:layout_marginLeft="20dp"
                    app:cardBackgroundColor="@color/white"
                    app:cardCornerRadius="9dp">
                    <ImageView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_marginBottom="28dp"
                        android:layout_marginTop="9dp"
                        android:src="@drawable/civil"
                        />

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:gravity="bottom"
                        android:text="Civil"
                        android:textAlignment="center"
                        android:textColor="@color/black"
                        android:textSize="22dp"
                        android:textStyle="bold"
                        tools:ignore="RtlCompat" />
                </androidx.cardview.widget.CardView>
            </LinearLayout>

            <LinearLayout
                android:id="@+id/linearsyllbus2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/club"
                android:layout_marginTop="8dp"
                android:layout_marginBottom="12dp"
                android:gravity="center"
                android:orientation="horizontal"
                android:paddingBottom="19dp">

                <androidx.cardview.widget.CardView
                    android:id="@+id/Mech"
                    android:layout_width="155dp"
                    android:layout_height="140dp"
                    app:cardBackgroundColor="@color/white"
                    app:cardCornerRadius="9dp">

                    <ImageView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_marginBottom="28dp"
                        android:layout_marginTop="9dp"
                        android:src="@drawable/mech"
                        />

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:gravity="bottom"
                        android:text="Mech"
                        android:textAlignment="center"
                        android:textColor="@color/black"
                        android:textSize="22dp"
                        android:textStyle="bold"
                        tools:ignore="RtlCompat" />
                </androidx.cardview.widget.CardView>

                <androidx.cardview.widget.CardView
                    android:id="@+id/EEE"
                    android:layout_width="155dp"
                    android:layout_height="140dp"
                    android:layout_marginLeft="20dp"
                    app:cardBackgroundColor="@color/white"
                    app:cardCornerRadius="9dp">

                    <ImageView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_marginBottom="28dp"
                        android:layout_marginTop="9dp"
                        android:src="@drawable/electrical"
                        />

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:gravity="bottom"
                        android:text="EEE"
                        android:textAlignment="center"
                        android:textColor="@color/black"
                        android:textSize="22dp"
                        android:textStyle="bold"
                        tools:ignore="RtlCompat" />
                </androidx.cardview.widget.CardView>


            </LinearLayout>


        </LinearLayout>
    </androidx.core.widget.NestedScrollView>
</RelativeLayout>